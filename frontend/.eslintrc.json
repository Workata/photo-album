{
    "env": {
        "browser": true,
        "node": true,
        "es2020": true
    },
    "extends": [
        "plugin:react/recommended",
        "standard"
    ],
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true,
            "modules": true
        },
        "ecmaVersion": 11,
        "sourceType": "module"
    },
    "plugins": [
        "react",
        "react-hooks"
    ],
    "rules": {

    },

    "settings": {
        "react": {
            "version": "detect"
        }
    }
}
